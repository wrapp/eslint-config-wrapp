{
  "name": "eslint-config-wrapp",
  "version": "16.1.0",
  "repository": "git@github.com:wrapp/eslint-config.git",
  "author": "emattias <mattiasha@gmail.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write '**/*.{js,jsx,graphql}'",
    "format:check": "prettier -l '**/*.{js,jsx,graphql}'",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "babel-eslint": "^8.2.2",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-compat": "^2.2.0",
    "eslint-plugin-eslint-comments": "^2.0.2",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.13.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "prettier": "^1.11.1"
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^5.0.0"
  }
}
